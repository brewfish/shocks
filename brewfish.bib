@article{lebel_innovation_2010,
  title = {Innovation Cycles, Niches and Sustainability in the Shrimp Aquaculture Industry in {{Thailand}}},
  volume = {13},
  issn = {14629011},
  abstract = {In this paper we explain how a shift from culture of the black tiger shrimp (Penaeus monodon) to the Pacific white shrimp (Litopenaeus vannamei) took place during 2002\textendash{}6 in Thailand. We use system innovation theory to help explain how niche substitution led to a regime change within a Thai aquaculture industry trying to maintain international competitiveness but under pressures from a global landscape in which consumers are increasingly concerned with health and ecological sustainability. Support from a vertically integrated major firm, an extensive foundation of learning networks within the industry, and early profitability made the scaling-up and embedding of the experiment with white shrimp very rapid once the formal ban on import of exotic broodstock was lifted. Disease management with domesticated, specific pathogen-free strains of white shrimp has proven much easier than with black shrimp still dependent on capture of wild broodstock. Moreover relative production costs are lower. The switch in species had significant consequences for the environment and firms. Using life cycle analysis we found that rearing white shrimp requires less resource and produces less waste than black shrimp. The shift in regime, however, also made it more difficult for small farms and hatchery businesses.},
  language = {en},
  number = {4},
  journal = {Environmental Science \& Policy},
  doi = {10.1016/j.envsci.2010.03.005},
  author = {Lebel, Louis and Mungkung, Rattanawan and Gheewala, Shabbir H. and Lebel, Phimphakan},
  month = jun,
  year = {2010},
  pages = {291-302},
  file = {/Users/ferraro/Zotero/storage/YUVQ5A7Z/Lebel et al. - 2010 - Innovation cycles, niches and sustainability in th.pdf}
}

@article{abreu_shrimp_2011,
  title = {Shrimp Farming in Coastal {{Brazil}}: {{Reasons}} for Market Failure and Sustainability Challenges},
  volume = {54},
  issn = {0964-5691},
  shorttitle = {Shrimp Farming in Coastal {{Brazil}}},
  abstract = {In Brazil, farm shrimp production has remained constant over the last five years but exports have fallen sharply and almost all production is now consumed domestically. This situation exists despite adequate water resources, available labour and climatic conditions which allow three production cycles per year. This paper is based on an assessment of the shrimp industry at a critical time when exports had reached their maximum level and were about to fall precipitously. It examines the reasons for the shrimp export market failure and discusses sustainability challenges for the future. Data were collected through interviews with hatchery, farm and processing plant managers and other stakeholders in Rio Grande do Norte state, the largest Brazilian farmed shrimp producer. It was found that market failure was the result of inadequate industry planning, inappropriate public policy and a lack of coordinated actions among the various shrimp producers and stakeholders. The research demonstrates that sustainable Brazilian shrimp farming will require technological and management improvements, institutional changes and an appropriate environmental and social framework.},
  number = {9},
  journal = {Ocean \& Coastal Management},
  doi = {10.1016/j.ocecoaman.2011.06.012},
  author = {de Abreu, M{\^o}nica Cavalcanti S{\'a} and de Mattos, Paloma and Lima, Pedro Elder Silva and Padula, Antonio Domingos},
  month = sep,
