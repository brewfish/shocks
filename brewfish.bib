  doi = {10.1111/faf.12414},
  author = {Zhang, Wenbo and Liu, Min and de Mitcheson, Yvonne Sadovy and Cao, Ling and Leadbitter, Duncan and Newton, Richard and Little, David C. and Li, Songlin and Yang, Yi and Chen, Xiao and Zhou, Wei},
  year = {2019},
  keywords = {management,biodiversity,feed-grade fish,multispecies fisheries,trash fish,trawl},
  file = {/Users/ferraro/Zotero/storage/Z33NYPWJ/Zhang et al. - Fishing for feed in China Facts, impacts and impl.pdf;/Users/ferraro/Zotero/storage/2EEILNA9/faf.html}
